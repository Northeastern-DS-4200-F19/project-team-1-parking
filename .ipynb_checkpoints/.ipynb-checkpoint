    "df2['8:00 AM Unoccupied'] = df3['8:00 AM']\n",
    "df2\n",
    "\n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 58,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Occupancy(time):\n",
    "    df2 = all_parking_data.loc[all_parking_data[time] == 'Occupied'].groupby(['Route Number','Side of Street']).count()[[time]];\n",
    "    df3 = all_parking_data.loc[all_parking_data[time] == 'Unoccupied'].groupby(['Route Number','Side of Street']).count()[[time]];\n",
    "    df2 = df2.rename(columns = {time: time + ' Occupied'})\n",
    "    df2[time + ' Unoccupied'] = df3[time]\n",
    "    df2['Total Spots'] = df2[time + ' Unoccupied'] + df2[time + ' Occupied']\n",
    "    return df2;"
   ]
  },
